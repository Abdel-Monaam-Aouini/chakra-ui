{
  "name": "@chakra-ui/clickable",
  "version": "2.0.11",
  "description": "React hook and component that implements native button interactions",
  "keywords": [
    "chakra ui",
    "clickable",
    "tabbable",
    "button",
    "div button"
  ],
  "sideEffects": false,
  "author": "Segun Adebayo <sage@adebayosegun.com>",
  "homepage": "https://github.com/chakra-ui/chakra-ui#readme",
